config_version=5

[application]

config/name="Stillworld"
run/main_scene="res://game/game.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DebugMenu="*res://debug/debug_menu.tscn"
Autoload="*res://autoload.gd"
SimpleGrass="*res://addons/simplegrasstextured/singleton.tscn"
TransitionManager="*res://game/transition_manager/transition_manager.tscn"
GameSaver="*res://scripts/game_saver/game_saver.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/vsync/vsync_mode=0

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/simplegrasstextured/plugin.cfg")

