config_version=5

[application]

config/name="Stillworld"
run/main_scene="res://gui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

DebugMenu="*res://debug/debug_menu.tscn"
Autoload="*res://scripts/autoload.gd"
SimpleGrass="*res://addons/simplegrasstextured/singleton.tscn"
TransitionManager="*res://gui/transition_manager/transition_manager.tscn"
